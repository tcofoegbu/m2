<project>
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.andromda.documentation</groupId>
        <artifactId>andromda-documentation</artifactId>
        <version>3.3-SNAPSHOT</version>
    </parent>
    <artifactId>andromda-getting-started-java</artifactId>
    <packaging>pom</packaging>
    <name>AndroMDA Getting Started Java Documentation</name>
    <description>The AndroMDA getting started Java documentation which has been superceeded</description>
    <reporting>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-project-info-reports-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-changes-plugin</artifactId>
                <reportSets>
                    <reportSet>
                        <reports>
                            <report>changes-report</report>
                        </reports>
                    </reportSet>
                </reportSets>
            </plugin>
        </plugins>
    </reporting>
</project>
